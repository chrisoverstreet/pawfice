{
  "name": "pawfice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "predev": "pnpm db:start && pnpm db:generate && pnpm typesense:start",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:start": "pnpm dlx supabase@latest --network-id pawfice-net start",
    "db:generate": "mkdirp utils/supabase && pnpm dlx supabase@latest --network-id pawfice-net gen types typescript --local --schema public > utils/supabase/types.ts",
    "db:dump": "pnpm dlx supabase@latest db dump --local --data-only --file=supabase/seed.sql",
    "db:reset": "pnpm dlx supabase@latest --network-id pawfice-net db reset",
    "typesense:start": "cd typesense && docker-compose --env-file ../.env.local up -d",
    "typesense:reset": "tsx --env-file .env.local typesense/reset.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@supabase/auth-js": "^2.66.1",
    "@supabase/ssr": "^0.5.2",
    "@supabase/storage-js": "^2.7.1",
    "@supabase/supabase-js": "^2.46.2",
    "@tanstack/react-query": "^5.59.20",
    "@tanstack/react-query-devtools": "^5.59.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "invariant": "^2.2.4",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.11.14",
    "lucide-react": "^0.456.0",
    "next": "^15.0.4",
    "next-safe-action": "^7.9.9",
    "next-themes": "^0.4.3",
    "nuqs": "^2.1.1",
    "pluralize": "^8.0.0",
    "react": "^19.0.0",
    "react-day-picker": "8.10.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-easy-crop": "^5.1.0",
    "react-hook-form": "^7.53.2",
    "sonner": "^1.7.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "typesense": "^1.8.2",
    "use-debounce": "^10.0.4",
    "uuid": "^11.0.3",
    "vaul": "^1.1.1",
    "zod": "^3.23.8",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.60.1",
    "@types/invariant": "^2.2.37",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "mkdirp": "^3.0.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
